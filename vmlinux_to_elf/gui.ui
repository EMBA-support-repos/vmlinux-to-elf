<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.1 -->
<interface>
  <!-- interface-name gui.ui -->
  <requires lib="gio" version="2.0"/>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.4"/>
  <object class="AdwApplicationWindow" id="main_window">
    <child>
      <object class="AdwNavigationView">
        <child>
          <object class="AdwNavigationPage">
            <property name="title">vmlinux-to-elf</property>
            <child>
              <object class="AdwToolbarView">
                <property name="top-bar-style">raised</property>
                <child type="top">
                  <object class="AdwHeaderBar">
                    <child type="end">
                      <object class="GtkMenuButton">
                        <property name="icon-name">open-menu-symbolic</property>
                        <property name="popover">
                          <object class="GtkPopoverMenu">
                            <property name="menu-model">menu</property>
                          </object>
                        </property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesPage">
                    <property name="title">Main panel</property>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Welcome to vmlinux-to-elf. This tool serves the purpose of facilitating reverse engineering of Linux kernels found in embedded devices, generating symbol and/or ELF files suitable for tools such as IDA Pro.</property>
                            <property name="wrap">True</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <property name="description">Please select a kernel to analyse (*.vmlinux, *.vmlinuz, *.bzImage, *.zImage)</property>
                        <property name="title">Kernel selection</property>
                        <child>
                          <object class="AdwActionRow" id="file_picker_button">
                            <property name="activatable">True</property>
                            <property name="activatable-widget">file_picker_icon</property>
                            <property name="title">Select a file...</property>
                            <child>
                              <object class="GtkButton" id="file_picker_icon">
                                <property name="icon-name">document-open</property>
                                <style>
                                  <class name="flat"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow" id="kernel_string_row">
                            <property name="title">Detected kernel string</property>
                            <property name="visible">False</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <property name="description">vmlinux-to-elf will attempt to detect automatically the following parameters if not specified explictly.</property>
                        <property name="title">Analysis options</property>
                        <child>
                          <object class="AdwEntryRow">
                            <property name="text">Testt</property>
                            <property name="title">Test</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwEntryRow">
                            <property name="name">Test 2</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwComboRow">
                            <property name="subtitle">Architecture</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwSwitchRow"/>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <child type="end">
                          <object class="GtkButton">
                            <property name="halign">end</property>
                            <property name="label">Detect symbols</property>
                            <property name="sensitive">False</property>
                            <property name="valign">center</property>
                            <style>
                              <class name="pill"/>
                              <class name="suggested-action"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
  <menu id="menu">
    <item>
      <attribute name="label">About...</attribute>
    </item>
  </menu>
</interface>
